# Youth Green Jobs Hub - Frontend Environment Variables

# ===== API CONFIGURATION =====
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=v1
VITE_API_TIMEOUT_MS=30000

# ===== APP CONFIGURATION =====
VITE_APP_NAME="Youth Green Jobs Hub"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Connecting youth with green jobs and eco-friendly opportunities"

# ===== FEATURES =====
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_GEOLOCATION=true

# ===== GEOLOCATION CONFIGURATION =====
VITE_DEFAULT_LATITUDE=-0.0917
VITE_DEFAULT_LONGITUDE=34.7680
VITE_GEOLOCATION_TIMEOUT_MS=10000
VITE_GEOLOCATION_MAX_AGE_MS=300000
VITE_GEOLOCATION_HIGH_ACCURACY=true

# ===== UI CONFIGURATION =====
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_FILE_SIZE_MB=10
VITE_SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,webp
VITE_SUPPORTED_DOCUMENT_FORMATS=pdf,doc,docx

# ===== PLATFORM CONFIGURATION =====
VITE_DEFAULT_COUNTY=Kisumu
VITE_DEFAULT_COUNTRY=Kenya
VITE_PLATFORM_TIMEZONE=Africa/Nairobi

